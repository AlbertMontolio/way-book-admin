<%= render "categories-menu", id: division["id"], division_categories: division["categories"] %>

<div class="company_skills-block">
	<%= render "header", name: division["name"], id: division["id"] %>
	<% division["categories"].each do |category| %>
		
		<%= render "division-category-header", category_name: category["name"], division_id: division["id"], category_id: category["id"], category_name: category["name"] %>

		<div class="company_skills">
			<%= render "division_company_skills", category_company_skills: category["company_skills"], category_id: category["id"] %>
		</div>
	<% end %>
</div>


















